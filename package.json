{
  "name": "11ty",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4"
  },
  "scripts": {
    "clean": "rm -rf tmp/ dist/",
    "11ty:watch": "eleventy --input=src/ --output=tmp/ --watch",
    "11ty:build": "eleventy --input=src/ --output=tmp/",
    "parcel:serve": "parcel serve 'tmp/**/*.html'",
    "parcel:build": "parcel build 'tmp/**/*.html'",
    "predev": "run-s clean",
    "dev": "run-p 11ty:watch parcel:serve",
    "build": "run-s clean 11ty:build parcel:build"
  }
}
